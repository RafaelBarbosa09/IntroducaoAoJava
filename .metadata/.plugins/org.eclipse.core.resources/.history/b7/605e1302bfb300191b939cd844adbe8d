
public class TestaFuncionario {

	public static void main(String[] args) {
		
		Funcionario rafael = new Funcionario("Rafael", "13764247703", 1500 );
		
		System.out.println(rafael.getNome());
		System.out.println(rafael.getCpf());
		System.out.println(rafael.getSalario());
		System.out.println(rafael.getBonificacao());
		System.out.println("O total do salário com a bonificação é: " + (rafael.getSalario() + rafael.getBonificacao()));

	}

}
